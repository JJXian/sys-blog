tivityDetail?activityId=' + id)\n    },\n    load(pageNum) {  // 分页查询\n      if (pageNum) this.pageNum = pageNum\n      this.$request.get('/activity/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name,\n        }\n      }).then(res => {\n        this.tableData = res.data?.list\n        this.total = res.data?.total\n      })\n    },\n    reset() {\n      this.name = null\n      this.load(1)\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum)\n    },\n  }\n}\n</script>\n\n<style scoped>\n\n</style>"],"mappings":"AAUA,OAAAA,MAAA;AACA,OAAAC,YAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAF,YAAA;IACAD;EACA;EAEAI,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAN,IAAA;IACA;EACA;EACAO,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAC,SAAAC,EAAA;MACAC,MAAA,CAAAC,IAAA,uCAAAF,EAAA;IACA;IACAH,KAAAJ,OAAA;MAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAU,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAZ,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAL,IAAA,OAAAA;QACA;MACA,GAAAiB,IAAA,CAAAC,GAAA;QACA,KAAAf,SAAA,GAAAe,GAAA,CAAAhB,IAAA,EAAAiB,IAAA;QACA,KAAAb,KAAA,GAAAY,GAAA,CAAAhB,IAAA,EAAAI,KAAA;MACA;IACA;IACAc,MAAA;MACA,KAAApB,IAAA;MACA,KAAAQ,IAAA;IACA;IACAa,oBAAAjB,OAAA;MACA,KAAAI,IAAA,CAAAJ,OAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}